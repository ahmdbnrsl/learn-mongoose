{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "module": "es6",
        "noEmit": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "checkJs": true
    }
}
